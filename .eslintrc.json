{
    "env": {
        "commonjs": true,
        "es2020": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "globals": {
      "use": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 11
    },
    "rules": {
      "class-methods-use-this": "off",
      "consistent-return": "off",
      "no-param-reassign": "off"
    }
}
